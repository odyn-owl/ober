<h1>Menu items</h1>

<table>
  <tr>
  </tr>

<% for menu_item in @menu_items %>
  <tr>
    
    <td><%= link_to menu_item.name, menu_item %></td>
    <td><%= link_to 'Edit', edit_menu_item_path(menu_item) %></td>
    <td><%= link_to 'Destroy', menu_item, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New menu item', new_menu_item_path %>
